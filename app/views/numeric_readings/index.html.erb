<h1>Listing numeric_readings</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Value</th>
    <th>Sensor</th>
  </tr>

<% for numeric_reading in @numeric_readings %>
  <tr>
    <td><%=h numeric_reading.name %></td>
    <td><%=h numeric_reading.value %></td>
    <td><%=h numeric_reading.sensor_id %></td>
    <td><%= link_to 'Show', numeric_reading %></td>
    <td><%= link_to 'Edit', edit_numeric_reading_path(numeric_reading) %></td>
    <td><%= link_to 'Destroy', numeric_reading, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New numeric_reading', new_numeric_reading_path %>
